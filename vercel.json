{
  "version": 2,
  "buildCommand": "chmod +x build.sh && ./build.sh",
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.4",
      "excludeFiles": "storage/**,tests/**,.git/**,bootstrap/cache/**,node_modules/**,public/assets/**,resources/**,installation/**,Modules/**,admin-bancas-do-bairro/**,servidor-web/**,database/**,tools/**,*.log,*.cache,*.md,*.txt,composer.lock,package-lock.json,webpack.mix.js,.env.*,README.md,.editorconfig,.styleci.yml,.gitattributes,.gitignore,.htaccess,.rnd,phpunit.xml,artisan,server.php"
    }
  },
  "routes": [
    { "src": "/build/(.*)", "headers": {"Cache-Control": "public, max-age=31536000, immutable"}, "dest": "/public/build/$1" },
    { "src": "/storage/(.*)", "dest": "/public/storage/$1" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
