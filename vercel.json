{
  "version": 2,
  "buildCommand": null,
  "installCommand": null,
  "outputDirectory": null,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/api/index.php" },
    { "source": "/(.*)", "destination": "/api/index.php" }
  ],
  "headers": [
    {
      "source": "/api/test.html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}
