{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Minimal Laravel for Vercel deployment",
    "require": {
        "php": "^7.3|^8.1",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
